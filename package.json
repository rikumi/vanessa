{
  "name": "vanessa",
  "version": "1.4.0",
  "description": "An HTTPS MITM Proxy based on Koa.js",
  "main": "src/app.js",
  "repository": "https://github.com/rikumi/vanessa",
  "author": "rikumi <riku.mi@outlook.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node src/app.js",
    "install": "node src/install.js",
    "preuninstall": "node src/uninstall.js",
    "prepublishOnly": "cross-env NODE_ENV=prebuild node src/build.js"
  },
  "bin": {
    "vanessa": "src/app.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "brake": "^1.0.1",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "collect-all": "^1.0.3",
    "cross-env": "^5.2.0",
    "delay-stream": "^0.0.3",
    "duplexify": "^4.0.0",
    "glob": "^7.1.3",
    "http-proxy-agent": "^2.1.0",
    "https-detect": "^0.0.0",
    "https-proxy-agent": "^2.2.1",
    "iltorb": "^2.4.3",
    "into-stream": "^5.1.0",
    "ip": "^1.1.5",
    "is-root": "^2.1.0",
    "json-stringify-safe": "^5.0.1",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa2-static-middleware": "^1.0.3",
    "less": "^3.9.0",
    "mkdirp": "^0.5.1",
    "monaco-editor": "^0.16.2",
    "node-forge": "^0.8.2",
    "null-writable": "^1.0.0",
    "pac-proxy-agent": "^3.0.0",
    "parcel": "^1.12.3",
    "path-to-regexp": "^3.0.0",
    "querystring": "^0.2.0",
    "raw-body": "^2.4.0",
    "replacestream": "^4.0.3",
    "rimraf": "^2.6.3",
    "socks-proxy-agent": "^4.0.2",
    "user-startup": "^0.2.2",
    "vm2": "^3.8.1",
    "vue": "^2.6.10",
    "ws": "^6.2.1",
    "yaml": "^1.5.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@types/glob": "^7.1.1",
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.40",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^11.13.7",
    "@types/semaphore": "^1.1.0",
    "@types/ws": "^6.0.1",
    "@types/yaml": "^1.0.2",
    "@vue/component-compiler-utils": "^3.0.0",
    "autoprefixer": "^9.5.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "pug": "^2.0.3",
    "vue-hot-reload-api": "^2.3.3",
    "vue-template-compiler": "^2.6.10"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      [
        "module-resolver",
        {
          "alias": {
            "vue": "vue/dist/vue.common"
          }
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": true
    }
  },
  "browserslist": [
    "ios >= 8",
    "android >= 4.1",
    "ie >= 10",
    "defaults"
  ],
  "nodemonConfig": {
    "restartable": false,
    "ignore": [
      "frontend/*"
    ]
  }
}
